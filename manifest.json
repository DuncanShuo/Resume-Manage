{
    "manifest_version": 3,
    "name": "履歷整理",
    "description": "輸入資料並傳送到 Google Apps Script",
    "version": "1.0",
    "permissions": ["storage"],
    "host_permissions": [
      "https://script.google.com/*"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png"
      }
    }
  }
  